{
  "name": "fdpg-webapp",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "vite -c vite.config.app.ts --port 8080",
    "dev:print": "vite -c vite.config.print.ts ./src/print-module --port 8081",
    "build": "vue-tsc --noEmit && vite -c vite.config.app.ts build",
    "build:print": "vue-tsc --noEmit && vite -c vite.config.print.ts build && npm run rename-print-index",
    "rename-print-index": "node -e \"require('fs').rename('print-frontend/index.print.html', 'print-frontend/index.html', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "preview": "vite preview --port 8080",
    "test:unit": "vitest --coverage",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "i18n:build": "node ./src/scripts/build-lang-files.js && prettier --parser json-stringify --write src/locales/*.json",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "prebuild": "npm run i18n:build"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.2",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@headlessui/vue": "^1.7.22",
    "@vueuse/core": "^10.10.0",
    "axios": "^1.7.2",
    "bootstrap-icons": "^1.11.3",
    "element-plus": "^2.7.4",
    "element-theme-chalk": "^2.15.14",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "tslib": "^2.6.2",
    "vue": "^3.4.27",
    "vue-i18n": "^9.13.1",
    "vue-oidc-client": "^1.0.0-alpha.5",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@pinia/testing": "^0.1.3",
    "@playwright/test": "^1.44.1",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.0",
    "sass": "^1.77.4",
    "sass-loader": "^14.2.1",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^4.3.9",
    "vitest": "^0.32.2",
    "vue-tsc": "^1.6.5"
  },
  "engines": {
    "node": ">=18"
  }
}
